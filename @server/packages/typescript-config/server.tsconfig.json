{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Server",
  "compilerOptions": {
    "noUnusedLocals": true,
    "module": "es6",
    "target": "es5",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "strict": true
  },
  "exclude": ["node_modules"]
}
